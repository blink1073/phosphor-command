  "typescript": {
    "definition" : "lib/phosphor-command.d.ts"
  },
  "dependencies": {
    "phosphor": "0.3.0",
    "phosphor-widget": "^0.9.0",
    "phosphor-arrays": "^0.9.0",
    "phosphor-disposable": "^0.9.0",
    "phosphor-messaging": "^0.9.0",
    "phosphor-nodewrapper": "^0.9.0",
    "phosphor-properties": "^0.9.0",
    "phosphor-queue": "^0.9.0",
    "phosphor-signaling": "^0.9.0",
    "topsort": "^0.0.2"
  },
  "devDependencies": {
    "browserify-istanbul": "^0.2.1",
    "coveralls": "^2.11.4",
    "del": "^0.1.3",
    "dive": "^0.4.0",
    "dts-generator": "^1.5.0",
    "event-stream": "^3.1.7",
    "expect.js": "^0.3.1",
    "karma": "^0.13.9",
    "karma-browserify": "^4.3.0",
    "karma-coverage": "^0.5.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.2",
    "shelljs": "^0.5.3",
    "stylus": "^0.45.0",
    "typedoc": "^0.3.8",
    "typescript": "^1.5.3"
  },
  "scripts": {
    "clean": "rimraf lib && rimraf test/build && rimraf test/coverage",
    "build:src": "tsc --project src",
    "build:dts": "node scripts/dtsbundle.js",
    "build:examples": "node scripts/buildexamples.js",
    "build:test": "tsc --project test/src",
    "build": "npm run build:src && npm run build:dts && npm run build:test",
    "prepublish": "npm run build",
    "test": "mocha test/build/index.js",
    "test:firefox": "karma start --browsers=Firefox test/karma.conf.js",
    "test:chrome": "karma start --browsers=Chrome test/karma.conf.js",
    "test:ie": "karma start --browsers=IE test/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dwillmer/phosphor-command.git"
  },
  "files": [
    "lib/index.js",
    "lib/phosphor-command.d.ts"
  ],
  "keywords": [
    "command", "action", "menu", "solver"
  ],
  "author": "Dave Willmer <dave.willmer@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/dwillmer/phosphor-command/issues"
